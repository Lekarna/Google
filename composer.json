	"require-dev": {
		"jakub-onderka/php-parallel-lint": "~0.7",
		"nette/bootstrap": "^2.3",
		"nette/tester": "@dev"
	},
	"support": {
		"email": "it@lekarna.cz",
		"issues": "https://github.com/Lekarna/Google/issues"
	},
	"autoload": {
		"psr-0": {
			"Kdyby\\Google\\": "src/"
		},
		"classmap": [
			"src/Kdyby/Google/exceptions.php"
		]
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.1-dev"
		}
	}
}
